<div class="row">
    <div class="col-xs-3">
        {{dwcm-textfield
            label='fields.labels.preparation.life-stage'
            value=model.objectAttribute.lifeStage
        }}
    </div>
    <div class="col-xs-3">
        {{
            dwcm-select
            label='fields.labels.preparation.sex'
            value=model.objectAttribute.sex
            options=sexOptions
        }}
    </div>
    <div class="col-xs-3">
        {{dwcm-textfield
            type='number'
            label='component.preparation.individuals-count'
            value=model.objectAttribute.totalCount
        }}
    </div>
</div>
{{partial 'partial/preparation/list'}}