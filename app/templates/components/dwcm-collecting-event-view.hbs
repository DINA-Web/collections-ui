{{#if model.locality.latitude1}}
    {{#leaflet-map lat=model.locality.latitude1 lng=model.locality.longitude1 zoom=zoom}}
        {{tile-layer url="http://{s}.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png"}}

        {{#circle-marker-layer location=location radius=5}}
            <p>{{model.locality.localityName}}</p>
        {{/circle-marker-layer}}
    {{/leaflet-map}}
{{/if}}
<ul>
    <li>Insamlingsdatum: {{model.startDate}} | {{model.verbatimDate}}</li>
    <li>Metod: {{model.method}}</li>

    <li>
        Fyndplats: {{model.locality.localityName}}({{model.locality.latitude1}}, {{model.locality.longitude1}}),
         {{model.verbatimLocality}}
    </li>
</ul>
